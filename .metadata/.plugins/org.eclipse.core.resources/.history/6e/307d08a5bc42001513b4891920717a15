package Count;

import java.util.*;

public class HiddenMessage {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		level_1();
	}
	
	public static void level_1(){
		String text = "";
		String pattern = "AAGAAGTAA";
		System.out.println(FrequentWords.FrequentWordCount(text, pattern));
	}
	
	public static void level_2() {
		String text = "";
		int k = 1;
		System.out.println(FrequentWords.FrequentKCount(text, k));
	}
}
